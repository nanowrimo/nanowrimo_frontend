{{#if groupMembers}}
  Note: All dates are in Greenwich Mean Time
  {{#ember-sortable-table class="table table-condensed"}}
    <thead>
      <tr>
        <th>Homed?</th>
        <th>Location</th>
        <th>Last Event RSVP</th>
      </tr>
    </thead>

    <tbody>
      {{#each groupMembers as |u|}}
        <tr>
          <td>{{u.homed}}</td>
          <td>{{u.location}}</td>
          <td>{{u.rsvp}}</td>
        </tr>
      {{/each}}
    </tbody>
  {{/ember-sortable-table}}
{{else}}
  <NwLoading />
{{/if}}
