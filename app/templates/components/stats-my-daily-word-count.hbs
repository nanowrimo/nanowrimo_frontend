<div class="nano-subcard-face face-front filled-white">
  <div class="nano-subcard-header">
    <div class="corner-label">My Daily Word Count</div>
    <div class="nano-subcard-toggle" {{action "toggleSubcard"}}>
      <div style="{{cardPrimaryStyle}}">{{fa-icon "line-chart"}}</div>
      <div style="{{cardSecondaryStyle}}">{{fa-icon "bar-chart"}}</div>
    </div>
  </div>
  <div class="nano-subcard-content">
    {{high-charts content=chartData chartOptions=chartOptions theme=theme}}
  </div>
</div>