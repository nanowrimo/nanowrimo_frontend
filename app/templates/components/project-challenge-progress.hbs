<div class='challenge-progress'>
  
  <div class='goal-text'>
    {{#if goalIcon}}
      {{{goalIcon}}}
    {{/if}}
    {{#if winnerRoute }}
      <span class='winner-route'>
        {{#link-to winnerRoute}}<img src='/images/cards/golden-trophy.svg' alt="" /> Winner{{/link-to}}
      </span>
    {{/if}}
  </div>
  
  {{nano-progress-bar percent=percentCompleted}}

  <div class="left-right-links challenge-info">
    <div class='progress-text'>
      <span class="count {{if goalMet 'goal-met' ''}}">{{format-number projectChallenge.count}}</span>
      <span class='goal'> / {{format-number projectChallenge.goal}} WORDS </span>
    </div>
    <div class='challenge-start'>
      <span class="started">Started</span>
      <span class="date">{{formattedStart}}</span>
    </div>
  </div>





</div>
