<div class="nw-item-social">
  {{#each socialLinks as |link|}}
    <a href={{link.link}} class="social-button" aria-label={{link.service}} target="_blank">{{fa-icon link.service}}</a>
  {{/each}}
</div>
{{#each otherLinks as |link|}}
  <div class="nw-item-link">
    <a href={{link.link}} aria-label={{link.url}} target="_blank">{{link.url}}</a>
  </div>
{{/each}}
